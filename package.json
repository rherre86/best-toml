{
  "name": "best-toml",
  "displayName": "Best TOML",
  "description": "Einfache, moderne TOML-Unterstützung für VS Code: Syntax-Highlighting und Grundkonfiguration.",
  "version": "0.0.1",
  "publisher": "roberth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roberth/best-toml.git"
  },
  "homepage": "https://github.com/roberth/best-toml",
  "bugs": {
    "url": "https://github.com/roberth/best-toml/issues"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": ["Programming Languages"],
  "keywords": [
    "toml",
    "pyproject",
    "config",
    "syntax highlighting"
  ],
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "language-configuration.json",
    "syntaxes/**"
  ],
  "contributes": {
    "languages": [
      {
        "id": "toml",
        "aliases": ["TOML", "toml"],
        "extensions": [".toml"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "toml",
        "scopeName": "source.toml",
        "path": "./syntaxes/toml.tmLanguage.json"
      }
    ]
  }
}
