{
  "name": "best-toml",
  "displayName": "Best TOML",
  "description": "Lightweight TOML syntax highlighting for VS Code â€“ perfect for pyproject.toml, Cargo.toml and other TOML-based configs.",
  "version": "0.0.1",
  "publisher": "roberth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rherre86/best-toml.git"
  },
  "homepage": "https://github.com/rherre86/best-toml",
  "bugs": {
    "url": "https://github.com/rherre86/best-toml/issues"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "toml",
    "pyproject",
    "config",
    "syntax highlighting",
    "cargo",
    "poetry",
    "uv"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "icon": "icon.png",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "MARKETPLACE_DESCRIPTION.md",
    "language-configuration.json",
    "syntaxes/**",
    "icon.png",
    "images/**",
    ".vscodeignore"
  ],
  "contributes": {
    "languages": [
      {
        "id": "toml",
        "aliases": [
          "TOML",
          "toml"
        ],
        "extensions": [
          ".toml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "toml",
        "scopeName": "source.toml",
        "path": "./syntaxes/toml.tmLanguage.json"
      }
    ]
  }
}